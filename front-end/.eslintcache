nt => {\r\n        setValues({ ...values, error: false, [name]: event.target.value });\r\n    };\r\n\r\n    const clickSubmit = event => {\r\n        event.preventDefault();\r\n        setValues({ ...values, error: false, loading: true });\r\n        signin({ email, password }).then(data => {\r\n            if (data.error) {\r\n                setValues({ ...values, error: data.error, loading: false });\r\n            } else {\r\n               authenticate(data, () => {\r\n                setValues({\r\n                    ...values,\r\n                  redirectToReferrer: true\r\n                });\r\n               }\r\n\r\n               )\r\n            }\r\n        });\r\n    };\r\n\r\n    const signInForm = () => (\r\n        <form>\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Email</label>\r\n                <input onChange={handleChange('email')} type=\"email\" className=\"form-control\" value={email} />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Password</label>\r\n                <input onChange={handleChange('password')} type=\"password\" className=\"form-control\" value={password} />\r\n            </div>\r\n            <button onClick={clickSubmit} className=\"btn btn-primary\">\r\n                Submit\r\n            </button>\r\n        </form>\r\n    );\r\n\r\n    const showError = () => (\r\n        <div className=\"alert alert-danger\" style={{ display: error ? '' : 'none' }}>\r\n            {error}\r\n        </div>\r\n    );\r\n\r\n    const showLoading = () => (\r\n       loading && (<div className=\"alert alert-info\"><h2>Loading...</h2> </div>)\r\n    );\r\n\r\n    const redirectUser = () =>{\r\n        if(redirectToReferrer){\r\n            if(user && user.role===1) {\r\n                return <Redirect to=\"/admin/account\"/>;\r\n            }\r\n            else {\r\n                return <Redirect to=\"/user/account\"/>;\r\n            }\r\n        }\r\n    }\r\n    return (\r\n        <Layout\r\n            \r\n\r\n            title=\"\"\r\n            description=\"Please sign in below\"\r\n            className=\"container col-md-4 offset-md-4\"\r\n        >\r\n            {showLoading()}\r\n            {showError()}\r\n            {signInForm()}\r\n            {redirectUser()}\r\n        </Layout>\r\n    );\r\n};\r\n\r\nexport default Signin;\r\n","C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\core\\Home.js",[],"C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\core\\Menu.js",[],"C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\core\\Layout.js",[],"C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\auth\\index.js",[],"C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\config.js",[],"C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\auth\\privateRoute.js",["80"],"import React, { Component } from 'react'\r\nimport {Route, Redirect} from 'react-router-dom'\r\nimport {isAuthenticated} from './index'\r\n\r\n\r\nconst PrivateRoute = ({component: Component, ...rest}) => (\r\n    <Route {...rest}  render={props => isAuthenticated()  ? (\r\n        <Component {...props} />\r\n    ) : (\r\n        <Redirect to={{pathname: '/signin',\r\n    state: { from: props.location}\r\n     }}\r\n     />\r\n    )}\r\n/>\r\n);\r\n\r\nexport default PrivateRoute;","C:\\Users\\Humayun\\Top10Movies-Examensarbete\\front-end\\src\\auth\\adminRoute.js",["81"],"\r\nimport React, { Component } from \"react\";\r\nimport { Route, Redirect } from \"react-router-dom\";\r\nimport { isAuthenticated } from \"./index\";\r\n\r\nconst AdminRoute = ({ component: Component, ...rest }) => (\r\n    <Route\r\n        {...rest}\r\n        render={props =>\r\n            isAuthenticated() && isAuthenticated().user.role === 1 ? (\r\n                <Component {...props} />\r\n            ) : (\r\n                <Redirect\r\n      